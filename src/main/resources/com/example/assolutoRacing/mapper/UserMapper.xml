<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.example.assolutoRacing.Mapper.UserMapper">
  <insert id="insert">
    insert into user
    (
     user_name,
     password,
     create_time,
     update_time
    )
    values(
     #{userName},
     #{password},
     NOW(),
     NOW()
    )
  </insert>
</mapper>